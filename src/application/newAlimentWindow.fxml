<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="newAlimentPane" prefHeight="278.0" prefWidth="123.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.newAlimentWindowController">
   <TextField fx:id="newAlimentSetName" alignment="CENTER" layoutX="14.0" layoutY="42.0" prefHeight="25.0" prefWidth="60.0" promptText="Name" />
   <TextField fx:id="newAlimentSetCal" alignment="CENTER" layoutX="14.0" layoutY="76.0" prefHeight="25.0" prefWidth="60.0" promptText="kcal" />
   <TextField fx:id="newAlimentSetFat" alignment="CENTER" layoutX="14.0" layoutY="110.0" prefHeight="25.0" prefWidth="60.0" promptText="Fat" />
   <TextField fx:id="newAlimentSetCarbs" alignment="CENTER" layoutX="14.0" layoutY="144.0" prefHeight="25.0" prefWidth="60.0" promptText="Carbs" />
   <TextField fx:id="newAlimentSetProtein" alignment="CENTER" layoutX="14.0" layoutY="177.0" prefHeight="25.0" prefWidth="60.0" promptText="Protein" />
   <TextField fx:id="newAlimentSetFiber" alignment="CENTER" layoutX="14.0" layoutY="210.0" prefHeight="25.0" prefWidth="60.0" promptText="Fiber" />
   <ToolBar prefHeight="34.0" prefWidth="123.0">
      <Label alignment="CENTER" prefHeight="15.0" prefWidth="72.0" text="New aliment" translateX="20.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </ToolBar>
   <Button fx:id="addButton" layoutX="14.0" layoutY="242.0" mnemonicParsing="false" onAction="#addAliment" text="Add" />
</Pane>
