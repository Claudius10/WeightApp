<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="app" minHeight="450.0" minWidth="550.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FoodTabController">
    <TabPane fx:id="mainWindow" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Tab fx:id="summaryTabLabel" text="Summary" />
        <Tab fx:id="foodTabLabel" closable="false" text="Food">
            <AnchorPane fx:id="foodTab" prefHeight="533.0" prefWidth="851.0">
                <AnchorPane fx:id="foodButtonBar" prefHeight="34.0" prefWidth="851.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <Label fx:id="alimentsLabel" alignment="CENTER" layoutX="7.0" layoutY="7.0" text="Aliments">
                        <font>
                            <Font name="System Bold" size="15.0" />
                        </font>
                    </Label>
                    <Button fx:id="deleteButton" layoutX="746.0" layoutY="5.0" mnemonicParsing="false" onAction="#deleteAliment" text="Delete" AnchorPane.rightAnchor="54.0" />
                    <Button fx:id="newButton" layoutX="798.0" layoutY="5.0" mnemonicParsing="false" onAction="#newAlimentWindow" text="New" AnchorPane.rightAnchor="8.0" />
                </AnchorPane>
                <TableView fx:id="alimentsTableView" layoutX="123.0" layoutY="34.0" prefHeight="236.0" prefWidth="851.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="34.0">
                    <columns>
                        <TableColumn fx:id="alimentsNameCol" prefWidth="141.0" text="Name" />
                        <TableColumn fx:id="alimentsKcalCol" prefWidth="141.0" text="Calories" />
                        <TableColumn fx:id="alimentsFatCol" prefWidth="141.0" text="Fat" />
                        <TableColumn fx:id="alimentsCarbsCol" prefWidth="141.0" text="Carbohydrate" />
                        <TableColumn fx:id="alimentsProteinCol" prefWidth="141.0" text="Protein" />
                        <TableColumn fx:id="alimentsFiberCol" prefWidth="144.0" text="Fiber" />
                    </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
                </TableView>
                <AnchorPane fx:id="mealButtonBar" layoutY="270.0" prefHeight="34.0" prefWidth="851.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <Label fx:id="mealsLabel" alignment="CENTER" layoutX="7.0" layoutY="7.0" text="Meals">
                        <font>
                            <Font name="System Bold" size="15.0" />
                        </font>
                    </Label>
                    <Button fx:id="newMealWindow" layoutX="798.0" layoutY="5.0" mnemonicParsing="false" onAction="#newMealWindow" text="New" />
                </AnchorPane>
            </AnchorPane>
        </Tab>
    </TabPane>
</AnchorPane>
